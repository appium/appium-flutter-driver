  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run test iOS",
      "env": {
        "APPIUM_OS": "ios"
      },
      "program": "${workspaceFolder}/example/src/index.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run test Android",
      "env": {
        "APPIUM_OS": "android"
      },
      "program": "${workspaceFolder}/example/src/index.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}